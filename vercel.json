{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "npm i -g pnpm && pnpm install",
  "buildCommand": "pnpm --filter=@flowveda/shared build && pnpm --filter=@flowveda/web build",
  "outputDirectory": "apps/web/dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}



















